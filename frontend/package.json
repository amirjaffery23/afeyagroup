{
  "name": "tailadmin-vue-free",
  "version": "1.0.3",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate-declarations && vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --skipLibCheck --noEmit",
    "generate-declarations": "vue-tsc --declaration --emitDeclarationOnly --outDir types",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@ag-grid-community/client-side-row-model": "^32.3.3",
    "@ag-grid-enterprise/column-tool-panel": "^32.3.3",
    "@ag-grid-enterprise/menu": "^32.3.3",
    "@vue/runtime-dom": "^3.5.13",
    "@vueuse/core": "^10.7.1",
    "ag-grid-community": "^32.3.3",
    "ag-grid-vue3": "^32.3.3",
    "apexcharts": "^4.1.0",
    "axios": "^1.7.8",
    "flatpickr": "^4.6.13",
    "jsvectormap": "^1.6.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5",
    "vue3-apexcharts": "^1.4.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
